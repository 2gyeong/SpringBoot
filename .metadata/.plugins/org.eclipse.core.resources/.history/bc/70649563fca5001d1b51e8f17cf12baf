package com.mysite.sbb2;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class HelloLombok1 {

	private String name;
	private int age;
	
	public static void main(String[] args) {

		// 객체 생성
		HelloLombok1 hl = new HelloLombok1();
		
		// Setter
		hl.setName("김이경");
		hl.setAge(28);
	}

}
